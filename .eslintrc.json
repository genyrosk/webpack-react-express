{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-unused-vars": 1,
    "camelcase": 0,
    "object-curly-newline": 0,
    "react/jsx-tag-spacing": 0
  }
}
